# CeiliClasses Environment Configuration

# API Configuration
API_BASE_URL=https://api.ceiliclasses.com
NODE_ENV=production

# App Configuration
APP_NAME=CeiliClasses
APP_VERSION=1.0.0
APP_BUILD_NUMBER=1

# Security Configuration
ENABLE_SECURITY_LOGGING=true
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true

# Cultural Content
DEFAULT_LANGUAGE=en
SUPPORTED_LANGUAGES=en,ga
CULTURAL_THEME=irish_heritage

# Development Only (DO NOT SET IN PRODUCTION)
# DEBUG_MODE=false
# ALLOW_HTTP=false
# DISABLE_SECURITY=false