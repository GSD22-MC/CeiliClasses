{
  "name": "ceiliclasses",
  "version": "1.0.0",
  "description": "Authentic Irish ceili dance learning app with cultural education",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "android:release": "react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "ios:release": "react-native run-ios --configuration Release",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android:bundle": "cd android && ./gradlew bundleRelease",
    "build:ios": "cd ios && xcodebuild -workspace CeiliClasses.xcworkspace -scheme CeiliClasses -configuration Release -destination generic/platform=iOS -archivePath CeiliClasses.xcarchive archive",
    "clean": "npx react-native clean-project-auto",
    "clean:ios": "cd ios && rm -rf build && rm -rf DerivedData",
    "clean:android": "cd android && ./gradlew clean",
    "security:audit": "npm audit && npm audit --audit-level high",
    "setup:platforms": "./scripts/setup-platforms.sh",
    "deploy:production": "./scripts/deploy-production.sh",
    "deploy:android": "cd android && ./gradlew assembleRelease",
    "deploy:android:bundle": "cd android && ./gradlew bundleRelease",
    "postinstall": "npx react-native-clean-project"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.0",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/bottom-tabs": "^6.5.0",
    "@react-navigation/stack": "^6.3.0",
    "react-native-screens": "^3.25.0",
    "react-native-safe-area-context": "^4.7.0",
    "react-native-gesture-handler": "^2.13.0",
    "react-native-video": "^5.2.1",
    "react-native-sound": "^0.11.2",
    "react-native-audio-recorder-player": "^3.6.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-svg": "^13.14.0",
    "@react-native-async-storage/async-storage": "^1.19.0",
    "react-native-progress": "^5.0.0",
    "react-native-modal": "^13.0.1",
    "react-native-linear-gradient": "^2.8.0",
    "axios": "^1.5.0",
    "react-query": "^3.39.0",
    "zustand": "^4.4.0",
    "react-native-paper": "^5.10.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-keychain": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.73.0",
    "@react-native/metro-config": "^0.73.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "keywords": ["irish", "ceili", "dance", "cultural", "education", "mobile"],
  "author": "CeiliClasses Team",
  "license": "MIT"
}